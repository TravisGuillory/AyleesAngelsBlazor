@page "/editpartner/{partnerId}"
@namespace AyleesAngels.Client.Pages






<AAHeader Heading="@ExistingPartner.Name" ></AAHeader>




<div class="editor">
    <input @bind=@ExistingPartner.Name placeholder="@ExistingPartner.Name" class="form-control m-1" />
    <textarea @bind=@ExistingPartner.ImageUrl placeholder="@ExistingPartner.ImageUrl" class="form-control m-1"></textarea>
    <textarea @bind=@ExistingPartner.LinkUrl placeholder="@ExistingPartner.LinkUrl" class="form-control m-1"></textarea>
    <button class="btn btn-primary m-1" @onclick="@UpdatePartner">Update Partner</button>
    <br />
</div>